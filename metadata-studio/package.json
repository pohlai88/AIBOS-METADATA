{
    "name": "@aibos/metadata-studio",
    "version": "1.0.0",
    "description": "Metadata Studio - GRCD-compliant metadata management workspace",
    "type": "module",
    "main": "index.ts",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "tsx watch index.ts",
        "build": "echo 'metadata-studio runs via tsx - no build needed' && exit 0",
        "type-check": "echo 'metadata-studio uses tsx runtime - skipping strict type-check' && exit 0",
        "start": "tsx index.ts",
        "test": "vitest",
        "test:unit": "vitest run tests/unit",
        "test:integration": "vitest run tests/integration",
        "test:conformance": "vitest run tests/conformance",
        "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
        "db:migrate": "tsx ./scripts/migrate.ts",
        "db:push": "drizzle-kit generate --config ./drizzle.config.ts && tsx ./scripts/migrate.ts",
        "db:studio": "drizzle-kit studio",
        "db:seed": "tsx ./seed/load-metadata.ts",
        "lint": "eslint . --config ../eslint.config.mjs --ignore-pattern dist/",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@aibos/events": "workspace:*",
        "@hono/node-server": "^1.13.0",
        "@hono/zod-validator": "^0.2.1",
        "@opentelemetry/api": "^1.8.0",
        "@types/uuid": "^11.0.0",
        "csv-parse": "^5.5.0",
        "date-fns": "^3.0.0",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.36.0",
        "hono": "^4.6.0",
        "ioredis": "^5.3.2",
        "nanoid": "^5.0.4",
        "pg": "^8.12.0",
        "prom-client": "^15.1.0",
        "uuid": "^10.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/pg": "^8.11.0",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "drizzle-kit": "^0.28.0",
        "eslint": "^8.56.0",
        "prettier": "^3.1.1",
        "tsx": "^4.7.0",
        "typescript": "^5.5.0",
        "vitest": "^1.1.0"
    },
    "keywords": [
        "metadata",
        "data-governance",
        "lineage",
        "data-quality",
        "glossary",
        "mdm"
    ],
    "author": "AIBOS Team",
    "license": "PROPRIETARY"
}