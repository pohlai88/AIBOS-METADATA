{
  "name": "@aibos/web",
  "version": "0.1.0",
  "dependencies": {
    "@aibos/metadata-studio": "workspace:*",
    "@aibos/types": "workspace:*",
    "@aibos/ui": "workspace:*",
    "@headlessui/react": "^2.2.9",
    "@neondatabase/serverless": "^1.0.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "next": "16.0.3",
    "postcss": "^8.5.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@aibos/config-eslint": "0.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.19.1",
    "@types/pg": "^8.15.6",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.6",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "private": true,
  "scripts": {
    "build": "pnpm exec next build --webpack",
    "dev": "pnpm exec next dev",
    "lint": "eslint . --config ../../eslint.config.mjs",
    "start": "pnpm exec next start",
    "db:seed": "tsx lib/seed.ts",
    "db:schema": "tsx -e \"import { readFileSync } from 'fs'; import { neon } from '@neondatabase/serverless'; const sql = neon(process.env.DATABASE_URL); const schema = readFileSync('lib/schema.sql', 'utf-8'); await sql(schema);\"",
    "db:tenant-id": "tsx lib/get-tenant-id.ts",
    "db:prepare-seed": "tsx lib/prepare-metadata-seed.ts",
    "db:seed-concepts": "tsx lib/seed-core-concepts.ts",
    "db:seed-aliases": "tsx lib/seed-aliases.ts",
    "db:seed-rules": "tsx lib/seed-rules.ts",
    "db:setup-accounting-knowledge": "tsx lib/setup-accounting-knowledge.ts"
  }
}